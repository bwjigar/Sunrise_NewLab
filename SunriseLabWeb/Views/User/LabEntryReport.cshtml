<input type="hidden" id="hdn_Id" />
<div class="container-fluid order-history-section manage-user">
    <input type="hidden" id="hdnDelUserId" />
    <div class="row">
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="order-title">
                <h2 id="h2_titl">Lab Entry Report</h2>
            </div>
        </div>
    </div>
    <div class="order-history-data admin muser gridview">
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
                <div class="col-xl-3 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                    <div class="order-date common-order">
                        <div class="offer-label">Lab Entry Date:</div>
                        <input type="text" name="From" id="txtFromDate" value="" class="offer-input offer-input1" />
                        <input type="text" name="To" id="txtToDate" value="" class="offer-input offer-input1" />
                    </div>
                </div>
                <div class="col-xl-5 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                    <div class="order-company-name common-order">
                        <div class="offer-label">Ref No:</div>
                        <input id="txt_S_StoneId" placeholder="Ref No / Lab No" type="text" class="form-control common-control" autocomplete="off">
                    </div>
                </div>
                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                    <div class="order-stone-id common-order">
                        <div class="offer-label">Cust. Name:</div>
                        <input id="txt_S_CustName" class="form-control common-control" placeholder="Customer Name / User Name" type="text" autocomplete="off">
                    </div>
                </div>
            </div>
            <div id="DivOption" class="col-xl-8 col-lg-8 col-md-8 col-sm-12 btn-filter" style="padding-left: 30px; margin-bottom: -13px;">
                <span class="btn-spn-opt" onclick="ActiveOrNot('ConfirmHold');" id="ConfirmHold">Confirm Hold</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Confirm');" id="Confirm">Confirm</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Hold');" id="Hold">Hold</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Bidded');" id="Bidded">Bidded</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Waiting');" id="Waiting">Waiting</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('QcPending');" id="QcPending">Qc Pending</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('QcReject');" id="QcReject">Qc Reject</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('BidReject');" id="BidReject">Bid Reject</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Sold');" id="Sold">Sold</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Transit');" id="Transit">Transit</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Busy');" id="Busy">Busy</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Cancel');" id="Cancel">Cancel</span>
                <span class="btn-spn-opt" onclick="ActiveOrNot('Other');" id="Other">Other</span>
            </div>
            <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12">
                <ul class="offer-inbtn pull-right mrtop src-rest-topbtn mt-2 mb-2">
                    <li>
                        <button class="offer-btn" onclick="GetSearch()"><i class="fa fa-search" aria-hidden="true"></i>Search</button>
                    </li>
                    <li>
                        <button class="offer-btn" onclick="Excel_LabEntryReport()"><i class="fa fa-file-excel-o" aria-hidden="true"></i>Excel</button>
                    </li>
                    <li>
                        <button class="offer-btn" onclick="Reset()"><i class="fa fa-refresh" aria-hidden="true"></i>Reset</button>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="aggrid-section gridview" style="margin-top: 0px;">
        <div class="row">
            <div class="col-xl-12">
                <div class="agGrid-box-main mt-2">
                    <div id="Cart-Gride" class="ag-theme-balham ag-fresh" style="float: left; width: 100%; height: calc(100vh - 225px);"></div>

                </div>
            </div>
        </div>
    </div>
</div>


