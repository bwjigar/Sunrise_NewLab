<input type="hidden" id="hdn_Id" />
<div class="container-fluid order-history-section">
    <div class="row">
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="order-title">
                <h2>Lab Entry</h2>
            </div>
        </div>
    </div>
    <div class="order-history-data admin">
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
                <div class="col-xl-2 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                    <div class="order-date common-order">
                        <div class="offer-label">Lab Date:</div>
                        <input type="text" name="txtLabDate" id="txtLabDate" value="" class="offer-input offer-input1" autocomplete="off" disabled>
                    </div>
                </div>
                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                    <div class="order-company-name common-order">
                        <div class="offer-label">User:</div>
                        <select id="ddl_User" class="form-control common-control" onchange="ddl_User_change()"></select>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                    <div class="order-stone-id common-order">
                        <div class="offer-label">Ref No / Cert No / Supplier Ref No:</div>
                        <input type="text" tabindex="2" class="form-control common-control" id="txtStoneId" maxlength="2000" autocomplete="off" placeholder="Ref No / Cert No / Supplier Ref No (Comma or Space)..." style="height: auto;" onkeyup="CommaSeperatedStone_list(event)">
                    </div>
                </div>
            </div>

            <div class="offset-7 col-xl-5 col-lg-4 col-md-5 col-sm-12">
                <ul class="offer-inbtn pull-right mrtop src-rest-topbtn mt-2 mb-2" style="margin: 20px 54px 0;">
                    <li>
                        <button class="offer-btn" onclick="GetSearch()"><i class="fa fa-search" aria-hidden="true"></i>Search </button>
                    </li>
                    <li>
                        <button class="offer-btn" onclick="Excel_LabEntry()"><i class="fa fa-file-excel-o" aria-hidden="true"></i>Excel </button>
                    </li>
                    <li>
                        <button class="offer-btn" onclick="Save_LabEntry()"><i class="fa fa-check" aria-hidden="true"></i>Lab Entry </button>
                    </li>
                    <li>
                        <button class="offer-btn" onclick="Reset()"><i class="fa fa-refresh" aria-hidden="true"></i>Reset </button>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="aggrid-section gridview">
        <div class="row">
            <div class="col-xl-12">
                <div class="agGrid-box-main mt-2">
                    <div id="Cart-Gride" class="ag-theme-balham ag-fresh" style="float: left; width: 100%; height: calc(100vh - 225px);"></div>
                </div>
            </div>
        </div>
    </div>
</div>

